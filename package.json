{
  "name": "vechain-dapp",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vechain/connex": "^2.0.12",
    "@vechain/connex-types": "^2.0.2",
    "antd": "^4.23.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.8",
    "thor-devkit": "^2.0.5",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "yarn && yarn contract:compile && yarn react-scripts start",
    "build": "yarn && yarn contract:compile && yarn react-scripts build",
    "test": "yarn && yarn contract:compile && yarn react-scripts test",
    "eject": "yarn react-scripts eject",
    "contract:compile": "yarn hardhat compile",
    "contract:deploy": "npx hardhat run --network vechain scripts/deploy.js",
    "lint": "yarn && yarn run eslint src",
    "format": "yarn prettier --write 'src'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@vechain.energy/hardhat-thor": "^0.3.1",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.1",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.7.1",
    "react-router-dom": "^6.4.2",
    "solidity-coverage": "^0.8.2",
    "tailwindcss": "^3.2.1",
    "typechain": "^8.1.0",
    "web3": "^1.8.0"
  }
}
